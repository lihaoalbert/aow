<tr id="<%= dom_id field %>" >
  <td><%= field.id %></td>
  <td><%= link_to field.label, field_path(field) %></td>
  <td><%= field.created_at %></td>
  <td>
    <%= link_to 'Edit', edit_field_path(field), :class => 'btn btn-mini', remote: true %>
    <%= link_to 'Destroy', field_path(field), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
  </td>
</tr>
